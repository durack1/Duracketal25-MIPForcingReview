	month = jan,
	year = {2020},
	pages = {201--224},
	file = {Full Text:/Users/durack1/Zotero/storage/U35AAFWS/Juckes et al. - 2020 - The CMIP6 Data Request (DREQ, version 01.00.31).pdf:application/pdf},
}

@article{balaji_requirements_2018,
	title = {Requirements for a global data infrastructure in support of {CMIP6}},
